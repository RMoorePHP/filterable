{
    "name": "rmoore/filterable",
    "description": "Filter eloquent models based on request params",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Moore",
            "email": "robert@sliske.uk"
        }
    ],
    "require": {
        "illuminate/database": "^6.0",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "php-coveralls/php-coveralls": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "RMoore\\Filterable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "RMoore\\Filterable\\ServiceProvider"
            ]
        }
    }
}
